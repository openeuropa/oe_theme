{#
/**
 * @file
 * Default template for the site navigation banner.
 *
 * Available variables:
 *   - site_name: the site name.
 *   - menu_items: menu items, must be compatible with "navigation_menu" pattern's "items" array.
 *
 * @ingroup themeable
 */
#}
{{ pattern('navigation_menu', {
  label: 'Menu'|t,
  close: close,
  items: menu_items,
  site_name: site_name,
  back: back|default(''),
  see_all_label: see_all_label|default(''),
  button_previous_label: button_previous_label|default(''),
  button_next_label: button_next_label|default(''),
  max_lines: max_lines|default(),
  variant: variant|default('default'),
}) }}
