{#
/**
 * @file
 * Template to display an inpage navigation block.
 */
#}
{% block navigation %}
  {# We add the extra attribute to guarantee the component is not auto initialized. #}
  {% include '@ecl-twig/inpage-navigation' with {
    'title': title,
    'icon_path': ecl_icon_path,
    'extra_classes': 'oe-theme-ecl-inpage-navigation ecl-u-z-dropdown',
    'extra_attributes': [
      {
        'name': 'data-ecl-auto-initialized',
        'value': 'true'
      }
    ]
  } %}
{% endblock %}
