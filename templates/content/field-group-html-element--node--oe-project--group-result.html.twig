{#
/**
 * @file
 * Project results fieldgroup html element.
 *
 * @see ./modules/contrib/field_group/templates/field-group-html-element.html.twig
 */
#}
<div id="project-results" class="ecl-u-mt-3xl">
  {% if title %}
    <h2 class="ecl-u-type-heading-2">{{ title|t }}</h2>
  {% endif %}
  <div class="ecl-row ecl-u-mb-l">
    <div class="ecl-col-m-6">
      {% block results %}
        {{ element.oe_project_results }}
      {% endblock %}
    </div>
  </div>
  {% block files %}
    {{ element.oe_project_result_files }}
  {% endblock %}
</div>
