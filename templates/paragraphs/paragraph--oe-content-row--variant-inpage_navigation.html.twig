{#
/**
 * @file
 * Theme override to display the "content row" type paragraph in the "inpage" variant.
 *
 * @see ./modules/contrib/paragraphs/templates/paragraph.html.twig
 */
#}
<div class="ecl-row">
  <div class="ecl-col-lg-3 ecl-u-z-navigation">
    {% include '@ecl-twig/twig-component-inpage-navigation/inpage-navigation.html.twig' with {
